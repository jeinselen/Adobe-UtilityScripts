<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.bpmMarkers" ExtensionBundleVersion="1.0.0" ExtensionBundleName="BPM Markers" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
